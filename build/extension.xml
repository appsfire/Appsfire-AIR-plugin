<?xml version="1.0" encoding="utf-8"?>
<extension xmlns="http://ns.adobe.com/air/extension/4.0">
    <id>com.appsfire.AppsfireANE</id>
    <versionNumber>1.3</versionNumber>
    <platforms>

        <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libAppsfireANE.a</nativeLibrary>
                <initializer>AppsfireANEInitializer</initializer>
                <finalizer>AppsfireANEFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="iPhone-x86">
            <applicationDeployment>
                <nativeLibrary>libAppsfireANE.a</nativeLibrary>
                <initializer>AppsfireANEInitializer</initializer>
                <finalizer>AppsfireANEFinalizer</finalizer>
            </applicationDeployment>
        </platform>

        <platform name="default">
            <applicationDeployment/>
        </platform>

    </platforms>
</extension>
